@page
@model ResetPasswordConfirmationModel
@{
    ViewData["Title"] = "Resetar Password Concluido";
}
>

<div class="min-vh-100 d-flex align-items-center  py-5 flex-column">
    <h1 class="fw-bold">Password Resetada com Sucesso! <img src="~/img/iconAnimeAlley.svg" alt="Alternate Text" /></h1>
    <p class="text-muted">
        Sua palavra passe foi resetada com sucesso! Por favor <a class="text-muted text-decoration-underline" asp-page="./Login">clique aqui para ir ao login.</a>
    </p>
</div>
